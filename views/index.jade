doctype html
html
  head
    title   

    //- meta tags
    meta(charset='utf-8')

    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    //- bootstrap css 
    link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.2/css/bootstrap.min.css" integrity="sha384-Smlep5jCw/wG7hdkwQ/Z5nLIefveQRIY9nfy6xoR1uRYBtpZgI6339F5dgvm/e9B" crossorigin="anonymous")

    //-  jQuery, Popper.js, bootstrap js respectively 

    script(src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous")

    script(src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous")

    script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.2/js/bootstrap.min.js" integrity="sha384-o+RDsa0aLu++PJvFqy8fFScvbHFLtbvScb8AjopnFD+iEQ7wo/CG0xlczd+2O/em" crossorigin="anonymous")
    
    //- owl-carousel css and JS
    link(rel="stylesheet", href='/external-lib/OwlCarousel2-2.3.4/dist/assets/owl.carousel.min.css')
    
    link(rel="stylesheet", href='/external-lib/OwlCarousel2-2.3.4/dist/assets/owl.theme.default.min.css')

    script(src='/external-lib/OwlCarousel2-2.3.4/dist/owl.carousel.min.js')

    link(rel="stylesheet", href="/stylesheets/style.css")
    link(rel="stylesheet", href="/stylesheets/schedule.css")
    link(rel="stylesheet", href="/stylesheets/bootstrap.min.js")  
    script(src='/javascripts/main.js')

  body
    //- main div
    div.container-fluid 
      div#myCarousel.owl-carousel.owl-theme.owl-loaded
        div.owl-stage-outer
          div.owl-stage
            div.owl-item 
              div.item 
                .container-fluid  
                  .text-center#heading-home PyBITS 2018
                  ul#nav.list-inline.text-center
                    li.list-inline-item
                      a(href="#") HOME &nbsp;|
                    li.list-inline-item 
                      a(href="#").schedule-tab SCHEDULE &nbsp;| 
                    li.list-inline-item 
                      a(href="#").talks-tab TALKS  &nbsp;|
                    li.list-inline-item 
                      a(href="#").workshops-tab WORKSHOPS  &nbsp;| 
                    li.list-inline-item 
                      a(href="#").code-of-conduct-tab CODE OF CONDUCT &nbsp;| 
                    li.list-inline-item 
                      a(href="#").about-tab ABOUT 
            div.owl-item
              div.item 
                #content
                    h1 Date
                    ul.timeline
                      li.event(data-date='Time')
                        h3 Event -1
                        p Event Details
                      li.event(data-date='Time')
                        h3 Event-2
                        p Event Details
                          
                      li.event(data-date='Time')
                        h3 Event -3
                        p Event Details
                      li.event(data-date='Time')
                        h3 Event - 4
                        p Event Details
                          
            div.owl-item
              div.item 
                h1 TALKS
            div.owl-item
              div.item 
                h1 WORKSHOPS
            div.owl-item
              div.item 
                h1 CODE OF CONDUCT
            div.owl-item
              div.item 
                h1 ABOUT

       

